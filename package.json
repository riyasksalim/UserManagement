{
  "scripts": {
    "build": "webpack",
    "start": "webpack-dev-server",
    "test": "./node_modules/.bin/jest"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test-shim.js",
      "<rootDir>/test-setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/test-preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  },
  "dependencies": {
    "awesome-typescript-loader": "^5.2.1",
    "mobx": "^5.14.0",
    "mobx-react": "^6.1.3",
    "node-fetch": "^2.6.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "source-map-loader": "^1.0.1",
    "tslib": "^1.10.0",
    "typescript": "^3.9.6",
    "whatwg-fetch": "^3.2.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^26.0.4",
    "@types/node": "^12.7.12",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/webpack": "^4.39.2",
    "@types/webpack-env": "^1.14.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.6.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^26.1.0",
    "react-test-renderer": "^16.13.1",
    "style-loader": "^1.2.1",
    "ts-loader": "^6.2.0",
    "ts-node": "^8.4.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  }
}